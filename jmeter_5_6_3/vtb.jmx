          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Create pause">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">int delta_current = (int)(Math.random() * 2 * vars.get(&quot;pacing_pause_delta&quot;).toInteger()) - vars.get(&quot;pacing_pause_delta&quot;).toInteger();
vars.put(&quot;pacing_pause_current&quot;, String.valueOf(vars.get(&quot;pacing_pause&quot;).toInteger() - delta_current));

Random random = new Random();
double value = Math.round(random.nextDouble() * 100 * 100) / 100.0;
vars.put(&quot;price&quot;, String.valueOf(value));

log.info(&quot;thread: &quot; + ${__threadNum} + &quot;, &quot; + &quot;iteration: &quot; + vars.getIteration() + &quot;, &quot; + &quot;pacing_pause: &quot; + vars.get(&quot;pacing_pause_current&quot;) + &quot;, &quot; + &quot;price: &quot; + vars.get(&quot;price&quot;));</stringProp>
